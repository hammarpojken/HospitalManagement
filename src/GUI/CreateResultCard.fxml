<BorderPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ResultCardController">
   <bottom>
      <Pane prefHeight="410.0" prefWidth="608.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="diseaseText" layoutX="41.0" layoutY="86.0" prefHeight="44.0" prefWidth="217.0" promptText="disease">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <TextField fx:id="medicineText" layoutX="41.0" layoutY="156.0" prefHeight="44.0" prefWidth="217.0" promptText="medicine">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <TextField fx:id="testText" layoutX="41.0" layoutY="235.0" prefHeight="44.0" prefWidth="217.0" promptText="test">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <TextArea fx:id="remarkArea" layoutX="323.0" layoutY="86.0" prefHeight="194.0" prefWidth="200.0" promptText="remark">
               <font>
                  <Font size="18.0" />
               </font>
            </TextArea>
            <Button fx:id="RSbtn" layoutX="230.0" layoutY="320.0" mnemonicParsing="false" onAction="#setResultCard" prefHeight="39.0" prefWidth="148.0" text="Create Result Card" />
