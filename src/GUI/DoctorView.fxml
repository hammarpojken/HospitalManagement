<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="DoctorViewPane" prefHeight="400.0" prefWidth="785.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.DoctorViewController">
   <left>
      <VBox prefHeight="400.0" prefWidth="123.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="CreateRSbtn" mnemonicParsing="false" onAction="#ShowResultCard" prefHeight="25.0" prefWidth="132.0" text="Create Result Card" />
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="130.0" text="Show Patients" />
            <Button mnemonicParsing="false" onAction="#ShowJournal" prefHeight="25.0" prefWidth="130.0" text="Show Journal" />
            <Button fx:id="appointmentbtn" layoutX="10.0" layoutY="60.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="130.0" text="Appointment" />
            <Button fx:id="assignbtn" layoutX="10.0" layoutY="85.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="130.0" text="Assign" />
         </children>
      </VBox>
   </left>
   <center>
      <HBox fx:id="DoctorHbox" prefHeight="400.0" prefWidth="677.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="tv" prefHeight="258.0" prefWidth="306.0">
               <columns>
                  <TableColumn fx:id="nameCol" prefWidth="75.0" text="First name" />
                  <TableColumn fx:id="lnameCol" prefWidth="75.0" text="Last name" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Separator orientation="VERTICAL" prefHeight="400.0" prefWidth="13.0" />
         </children>
      </HBox>
   </center>
</BorderPane>
