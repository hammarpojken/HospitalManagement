<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="PatientViewPane" prefHeight="400.0" prefWidth="785.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <VBox prefHeight="400.0" prefWidth="130.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" prefWidth="200.0" text="Personal Information" />
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefWidth="1800.0" text="Pay bills" />
         </children></VBox>
   </left>
   <center>
      <HBox prefHeight="400.0" prefWidth="677.0" BorderPane.alignment="CENTER">
         <children>
            <VBox fx:id="patientVbox" prefHeight="200.0" prefWidth="4000.0">
               <children>
                  <Label prefHeight="35.0" prefWidth="4000.0" style="-fx-alignment: CENTER;" text="Latest result card" textAlignment="CENTER">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <BorderPane>
                     <bottom>
                        <Pane maxWidth="476.0" prefHeight="303.0" prefWidth="476.0" BorderPane.alignment="CENTER">
                           <children>
                              <TextField fx:id="diseaseText" layoutX="14.0" layoutY="86.0" prefHeight="44.0" prefWidth="217.0" promptText="disease">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </TextField>
                              <TextField fx:id="medicineText" layoutX="14.0" layoutY="161.0" prefHeight="44.0" prefWidth="217.0" promptText="medicine">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </TextField>
                              <TextField fx:id="testText" layoutX="14.0" layoutY="236.0" prefHeight="44.0" prefWidth="217.0" promptText="test">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </TextField>
                              <TextArea fx:id="remarkArea" layoutX="265.0" layoutY="86.0" prefHeight="194.0" prefWidth="200.0" promptText="remark">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </TextArea>
                           </children>
                        </Pane>
                     </bottom>
                  </BorderPane>
               </children>
            </VBox>
         </children></HBox>
   </center>
</BorderPane>
